<span class="title">Dogstargram</span>
<h2><%= data.name %></h2>
<form method="GET" action="/home/<%= data._id %>">
    <div style="text-align: center;">
        <label for="name">Photo name:</label>
        <input type="name" name="name">
        <label for="date">date(ex 2020-04-12):</label>
        <input type="date" name="date">
        <input type="submit" value= "Filter"> 
        <input type="button" class="btn" onclick=" location.href='/others'" value="Look Other user's photo"/>     
    </div>
        
</form>
<% for(var i = 0; i < photos.length; i++){ %>
  
        
    <img src = "<%= photos[i].url %>" width="250" height="250"  style= " display:block"/> name: <%= photos[i].name %> date : <%= photos[i].date %>
    

<% } %>



<p><a href="/home/<%= data._id %>/add"><button>Add photo</button></a>
<a href="/home/"><button>Back</button></a>
<input type="button" class="btn" onclick=" location.href='/logout'" value="Log out"/>
</p>



